{% for invitado in invitados %}
  {% set inv = invitado['invitados'] %}
  {% set invitacion_aceptada = invitado['invitacion'] %}
  <tr>
    <td>{{ inv.nombre }}</td>
    <td>{{ inv.apellido }}</td>
    <td>{{ inv.fechaNac }}</td>
    <td>{{ inv.tipoDoc }}</td>
    <td>{{ inv.doc }}</td>
    <td>{{ invitacion_aceptada.proyecto.escuela }} </td>
    <td>{{ invitacion_aceptada.proyecto.escuela.localidad }} </td>
  </tr>
{% endfor %}

