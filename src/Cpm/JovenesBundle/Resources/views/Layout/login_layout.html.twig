<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>{% block title %}Bienvenido al Programa Jóvenes y Memoria de CPM{% endblock %}</title>
	{% block stylesheets %}{% endblock %}
	<link rel="stylesheet" href="{{ asset('css/screen.css') }}" />
	<link rel="stylesheet" href="{{ asset('jquery/jquery-ui-1.10.3.min.css') }}" />
	
	 <script src="{{ asset('jquery/jquery-1.10.2.min') }}" type="text/javascript"></script>
	 <script src="{{ asset('jquery/jquery-ui-1.10.3.min.js') }}" type="text/javascript"></script>
	 
	 <script src="{{ asset('js/analytics.js') }}" type="text/javascript"></script>
	<link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
</head>

<body class="login-layout">
<div class="clearfix" id="header-top">
		<div id="header-container">
			<a href="http://jovenesymemoria.comisionporlamemoria.net"> <img id="logo" src="{{ asset('images/login/logo.png') }}"></a>
			
			<div id="cat-nav">
				<div id="cat-nav-left"></div>
				<div id="cat-nav-content">

					<ul class="navi">
						<li><a href="{{ asset('files/instructivo.pdf') }}">Instructivo</a></li>
						<li><a href="{{path('fos_user_security_login')}}">Iniciar sesión</a></li>
						<li><a href="{{path('fos_user_registration_register')}}">Registrarse</a></li>
						<li><a href="{{path('fos_user_resetting_request')}}">Recuperar Clave</a></li>
						<li><a href="http://jovenesymemoria.comisionporlamemoria.net">Volver al sitio principal</a></li>
						
					</ul>
				</div>
				<!-- end #cat-nav-content -->
				<div id="cat-nav-right"></div>
			</div>
			<!-- end #cat-nav -->
		</div>
		<!-- end .container -->
	</div>
	<!-- end #header-top -->
	<div id="login-content">
		<div id="body-container">
			{% for key, message in app.session.getFlashes() %}
				<div class="{{key}}-message">{{ message|trans({}) }}</div>
			{% endfor %}
			<div class="login-form-title">
		      <h1>{{ block('title') }}</h1>
		   </div>
			
			{% block content %}{% endblock %}
 		</div>
 		<div class="push-footer"></div>
 		
	</div>
	<div id="footer-bottom">
		<div class="clearfix">
			&nbsp;
		</div>
			{% include 'CpmJovenesBundle:Layout:footer.html.twig' %}
	</div>
	
	
	
	
	{% block javascripts %}{% endblock %}

		<script>
			$(function() {
				$('.submit').button();
				$('#_submit').button();
				$('input[type=submit]').button();
				$('#login-extra-options li a').button();

			});
		</script>
	
</body>
</html>