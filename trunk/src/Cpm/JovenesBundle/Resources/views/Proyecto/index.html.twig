<h1>Proyecto list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Titulo</th>
            <th>Nroalumnos</th>
            <th>Esprimeravezdocente</th>
            <th>Esprimeravezescuela</th>
            <th>Esprimeravezalumnos</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('proyecto_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.titulo }}</td>
            <td>{{ entity.nroAlumnos }}</td>
            <td>{{ entity.esPrimeraVezDocente }}</td>
            <td>{{ entity.esPrimeraVezEscuela }}</td>
            <td>{{ entity.esPrimeraVezAlumnos }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('proyecto_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('proyecto_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('proyecto_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
