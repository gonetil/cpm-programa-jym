{% extends '::base.html.twig' %}

{% block content %}

<h1>Distrito</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Distrito</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Region Educativa</th>
            <td>{{ entity.region.nombre }}</td>
        </tr>
        
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('distrito') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('distrito_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('distrito_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

    
{% endblock %}
