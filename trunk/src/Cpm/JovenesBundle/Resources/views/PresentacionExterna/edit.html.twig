{% extends 'CpmJovenesBundle:Layout:admin_layout.html.twig' %}

{% block content %}
<h1>Editar Presentacion Externa</h1>

<form action="{{ path('presentacionexterna_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Guardar</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('presentacion') }}">
            Cancelar
        </a>
    </li>
</ul>
{% endblock %}
