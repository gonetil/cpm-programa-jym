<h1>PresentacionExterna list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Titulo</th>
            <th>Escuela</th>
            <th>Provincia</th>
            <th>Localidad</th>
            <th>Coordinador</th>
            <th>Personas confirmadas</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('presentacionexterna_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.titulo }}</td>
            <td>{{ entity.escuela }}</td>
            <td>{{ entity.provincia }}</td>
            <td>{{ entity.localidad }}</td>
            <td>{{ entity.getApellidoCoordinador() }}, {{ entity.getNombreCoordinador() }}</td>
            <td> {{ entity.personasConfirmadas }} </td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('presentacionexterna_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('presentacionexterna_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('presentacionexterna_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
