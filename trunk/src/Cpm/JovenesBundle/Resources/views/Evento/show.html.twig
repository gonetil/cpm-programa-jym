<h1>Evento</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Titulo</th>
            <td>{{ entity.titulo }}</td>
        </tr>
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
        <tr>
            <th>Pedirnumeroasistentes</th>
            <td>{{ entity.pedirNumeroAsistentes }}</td>
        </tr>
        <tr>
            <th>Permitirsuplente</th>
            <td>{{ entity.permitirSuplente }}</td>
        </tr>
        <tr>
            <th>Ofrecerhospedaje</th>
            <td>{{ entity.ofrecerHospedaje }}</td>
        </tr>
        <tr>
            <th>Ofrecerviaje</th>
            <td>{{ entity.ofrecerViaje }}</td>
        </tr>
        <tr>
            <th>Permitirobservaciones</th>
            <td>{{ entity.permitirObservaciones }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('evento') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('evento_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('evento_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
