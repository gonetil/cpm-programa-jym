	"Coordinador",
	"Distrito",
	"Localidad",
	"Escuela",
	"Personas confirmadas",
	"Auditorio",
	"Dia",
	"Bloque",
	"Hora de inicio",
\n
{% for presentacion in presentaciones %}
	"{{ presentacion.getApellidoCoordinador() }} {{ presentacion.getNombreCoordinador() }}",
	"{{ presentacion.distrito }}",
	"{{ presentacion.localidad }}",
	"{{ presentacion.escuela }}",
	"{{ presentacion.getPersonasConfirmadas() }}",
	{% if presentacion.bloque  %}
		"{{ presentacion.bloque.auditorioDia.auditorio.nombre }}",
		"{{ presentacion.bloque.auditorioDia.dia.numero }}",
		"{{ presentacion.bloque.nombre }}",
		"{{ presentacion.bloque.horaInicio | date('H:i')}}"
	{% else %}
	"Sin asignar"
	{% endif %}
{{ ' \n\r'|raw }}
{% endfor %}
