<h1>Escuela list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Otrotipoinstitucion</th>
            <th>Dequesetrata</th>
            <th>Motivorealizacion</th>
            <th>Impactobuscado</th>
            <th>Email</th>
            <th>Telefono</th>
            <th>Domicilio</th>
            <th>Codigopostal</th>
            <th>Director</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('escuela_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.otroTipoInstitucion }}</td>
            <td>{{ entity.deQueSeTrata }}</td>
            <td>{{ entity.motivoRealizacion }}</td>
            <td>{{ entity.impactoBuscado }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.telefono }}</td>
            <td>{{ entity.domicilio }}</td>
            <td>{{ entity.codigoPostal }}</td>
            <td>{{ entity.director }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('escuela_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('escuela_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('escuela_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
